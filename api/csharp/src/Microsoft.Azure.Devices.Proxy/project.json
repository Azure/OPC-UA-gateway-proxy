{
  "version": "0.0.6-*",
  "name": "Microsoft.Azure.Devices.Proxy",
  "title": "Microsoft.Azure.Devices.Proxy",
  "description": "Microsoft IoT Proxy Module for Field Gateway",
  "authors": [ "Microsoft" ],
  "buildOptions": {
    "xmlDoc": false,
    "allowUnsafe": true,
    "warningsAsErrors": false,
    "publicSign": false,
    "define": [ "TRACE" ]
  },
  "packOptions": {
    "tags": [ "Azure", "IoT", ".NET", "Relay", "websocket", "Proxy" ],
    "projectUrl": "https://github.com/Azure/azure-iot-proxy",
    "licenseUrl": "https://raw.githubusercontent.com/Azure/azure-iot-proxy/master/LICENSE",
    "releaseNotes": "https://github.com/Azure/azure-iot-proxy/releases",
    "requireLicenseAcceptance": true
  },
  "dependencies": {
    "Microsoft.Azure.Relay": "0.1.3-preview",
    "MsgPack.Cli": "0.9.0-beta1",
    "NETStandard.Library": "1.6.0",
    "Newtonsoft.Json": "9.0.1"
  },
  "frameworks": {
    "net46": {
      "dependencies": {
        "WindowsAzure.ServiceBus": "3.4.3"
      }
    },
    "net45": {
      "dependencies": {
        "WindowsAzure.ServiceBus": "3.4.3"
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "System.Runtime.Serialization.Json": "4.3.0",
        "system.xml.xpath.xmldocument": "4.3.0",
        "System.Xml.XmlDocument": "4.3.0",
        "System.Reflection.TypeExtensions": "4.3.0"
      }
    }
  }
}
