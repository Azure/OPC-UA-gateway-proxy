{
    "version": "0.0.7",
    "name": "Microsoft.Azure.Devices.Proxy",
    "title": "Microsoft Azure Devices Proxy API",
    "description": "Managed Service API for the Field Gateway Proxy Module",
    "authors": [ "microsoft" ],
    "buildOptions": {
        "xmlDoc": false,
        "allowUnsafe": true,
        "warningsAsErrors": true,
        "nowarn": [ "1591", "1734" ],
        "define": [ "TRACE" ]
    },
    "packOptions": {
        "tags": [ "Azure", "IoT", ".NET", "Relay", "websocket", "Proxy" ],
        "projectUrl": "https://github.com/Azure/iot-gateway-proxy",
        "licenseUrl": "https://raw.githubusercontent.com/Azure/iot-gateway-proxy/master/LICENSE",
        "releaseNotes": "https://github.com/Azure/iot-gateway-proxy/releases",
        "requireLicenseAcceptance": true,
        "repository": {
            "url": "https://github.com/Azure/iot-gateway-proxy"
        }
    },
    "dependencies": {
        "Microsoft.Azure.Relay": "0.1.4-*",
        "NETStandard.Library": "1.6.1",
        "Newtonsoft.Json": "9.0.1"
    },
    "frameworks": {
        "net46": {
            "dependencies": {
                "WindowsAzure.ServiceBus": "3.4.5"
            }
        },
        "net45": {
            "dependencies": {
                "WindowsAzure.ServiceBus": "3.4.5"
            }
        },
        "netstandard1.3": {
            "dependencies": {
                "System.Runtime.Serialization.Json": "4.3.0",
                "system.xml.xpath.xmldocument": "4.3.0",
                "System.Xml.XmlDocument": "4.3.0",
                "System.Reflection.TypeExtensions": "4.3.0"
            }
        }
    },
    "configurations": {
        "Signed": {
            "buildOptions": {
                "define": [ "RELEASE_DELAY_SIGN" ],
                "optimize": true
            }
        },
        "Release": {
            "buildOptions": {
                "optimize": true
            }
        }
    }
}
